right = [['R-1', 'R-2', 'R-3', 'R-4', 'R-5', 'R-6', 'R-7', 'R-8', 'R-9', 'R-10', 'R-11', 'R-12', 'R-13', 'R-14', 'R-15', 'R-16', 'R-17', 'R-18', 'R-19', 'R-20', 'R-21', 'R-22', 'R-23', 'R-24', 'R-25'], ['313000', '両側', '3001', '腫瘤', '4001', '単発', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', ''], ['', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', ''], ['311000', '右', '3002', '石灰化', '', '', '', '', '5103', '多形性', '5205', '集簇性', '', '', '', '', '', '', '', '', '', '', '', '', '', '']]
left = [['L-1', 'L-2', 'L-3', 'L-4', 'L-5', 'L-6', 'L-7', 'L-8', 'L-9', 'L-10', 'L-11', 'L-12', 'L-13', 'L-14', 'L-15', 'L-16', 'L-17', 'L-18', 'L-19', 'L-20', 'L-21', 'L-22', 'L-23', 'L-24', 'L-25'], ['312000', '左', '3002', '石灰化', '', '', '', '', '5101', '微小円形', '5201', '散在性', '', '', '', '', '', '', '', '', '', '', '', '', '', ''], ['', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', ''], ['312000', '左', '3002', '石灰化', '', '', '5002', '血管', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '']]

with open (original,"r", encoding="SJIS") as f:
    rows = csv.reader(f)
# 1-28項目のデータを取り出してfilteredに格納
    for row in rows:
        filtered_row = row[:28]  # 1-28項目を取り出す
        filtered.append(filtered_row)
        print(filtered)
    for i in range(len(filtered)):
        if i < len(right):
            filtered[i].extend(right[i])

print(filtered)


with open(original, "w", newline="", encoding="SJIS") as i:
    csv.writer(i).writerows(filtered)

