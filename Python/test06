list_of_lists = [
    ["right", 100, "right", 101, "left", 102],
    ["left", 200, "right", 201, "birateral", 203],
    ["right", 300, "birateral", 301, "left", 303]
]

results = []

for lst in list_of_lists:
    R = None
    L = None
    i = 0

    while i < len(lst):
        item = lst[i]

        if item == "right" and R is None:
            R = lst[i+1]
            i += 2
        elif item == "left" and L is None:
            L = lst[i+1]
            i += 2
        elif item == "birateral" and R is None:
            R = lst[i+1]
            i += 2
        elif item == "birateral" and L is None:
            L = lst[i+1]
            i += 2
        else:
            i += 2

    results.append((R, L))

for index, (R, L) in enumerate(results, start=1):
    print(f"List {index}:")
    print("R:", R)
    print("L:", L)
    print()